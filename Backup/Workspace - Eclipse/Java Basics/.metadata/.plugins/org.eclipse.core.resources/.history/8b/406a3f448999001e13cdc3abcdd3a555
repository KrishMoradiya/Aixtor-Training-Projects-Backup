package com.Aixtor.Krish_Moradiya.Basics;

class OperatorPrecedence {
	public static void main(String[] args) {
		int a = 25;
		int b = 5;
		System.out.println( 25 + 5 - (10-5));
		System.out.println( b-- + 25 + b++ - 10 - 5 );
		System.out.println( --b + 25 + ++b - 18/3 - 5 );
		System.out.println( 25 + 5 - (10-5));
		System.out.println( 25 + 5 - (10-5));
	}
}
