package com.Aixtor.OopsConcepts;
class A{
	A(){
		System.out.println("This is A Class..");
	}
	private void thisClassMethod() {
		System.out.println("this is the method of this class...");
	}
	public void ruleOfThis() {
		System.out.println(this);
		
		System.out.println(this);
		System.out.println(this);
		System.out.println(this);
		
	}
}
