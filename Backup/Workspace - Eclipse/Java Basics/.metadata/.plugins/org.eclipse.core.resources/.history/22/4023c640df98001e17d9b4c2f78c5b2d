package com.Aixtor.Krish_Moradiya.Basics;

public class Variables {

	public static void main(String[] args) {
		int i = 25;
		boolean b = true; // default: false
		float f = 25.0f;
		short s = 9999;
		byte bt = 127;
		long l = 14000l;
		double d = 3500.0;
		char c = 'K';
		
//		System.out.println(i);
//		System.out.println(b);
		
	}
	

}
